{
  "name": "easycanvas",
  "version": "0.2.8",
  "description": "A cute canvas framework. See https://chenzhuo1992.github.io/easycanvas",
  "keywords": "canvas, framework, animation",
  "main": "build/easycanvas.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:chenzhuo1992/easycanvas.git"
  },
  "author": "chenzhuo04",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6",
    "babel-loader": "6",
    "babel-plugin-add-module-exports": "0.2.*",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-assign": "6",
    "babel-preset-es2015": "6",
    "babel-runtime": "6",
    "concurrently": "2.2.0",
    "ejs-compiled-loader": "2",
    "html-webpack-plugin": "^2.30.1",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-requirejs": "^1.1.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.5",
    "node-sass": "^4.5.3",
    "requirejs": "^2.3.5",
    "sass-loader": "^6.0.6",
    "webpack": "1",
    "webpack-dev-server": "1"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "b": "npm run ec:build",
    "d": "npm run ec:dist",
    "g": "npm run ec:debug",
    "u": "npm run karma:unit",
    "r": "npm run readme",
    "dev": "npm run ec:dev",
    "doc": "npm run ec:doc",
    "readme": "node ./readme/language_deliver.js",
    "ec:build": "rm -rf ./build && (concurrently 'webpack --config ./webpack/webpack.easycanvas.min.js' 'webpack --config ./webpack/webpack.easycanvas.standalone.dev.js' 'webpack --config ./webpack/webpack.easycanvas.standalone.prod.js') && echo done.",
    "ec:debug": "webpack --config ./webpack/webpack.easycanvas.local.js",
    "ec:dist": "npm run ec:build",
    "ec:dev": "webpack-dev-server --config ./webpack/webpack.easycanvas.dev.js",
    "ec:doc": "webpack-dev-server --config ./webpack/webpack.easycanvas.docs.js",
    "karma:unit": "karma start ./karma/karma.conf.js"
  }
}
